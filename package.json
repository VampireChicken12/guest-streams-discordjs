{
	"name": "guest-streams-discordjs",
	"version": "1.0.0",
	"description": "A discord bot to let channel guests view and start streams!",
	"main": "./dist/index.js",
	"type": "commonjs",
	"scripts": {
		"start": "node .",
		"dev": "ts-node-dev --inspect --poll --respawn --ignore-watch node_modules --transpile-only -r tsconfig-paths/register ./src/index.ts",
		"build": "tsc",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@discordjs/rest": "^1.7.1",
		"@hapi/bourne": "^3.0.0",
		"@total-typescript/ts-reset": "^0.4.2",
		"discord.js": "^14.11.0",
		"dotenv": "^16.0.3",
		"joi": "^17.7.0",
		"module-alias": "^2.2.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.66",
		"@types/module-alias": "^2.0.4",
		"@types/node": "^18.15.11",
		"@typescript-eslint/eslint-plugin": "^5.58.0",
		"@typescript-eslint/parser": "^5.58.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.0"
	},
	"_moduleAliases": {
		"@": "./dist"
	},
	"author": "VampireChicken12",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/VampireChicken12/guest-streams-discordjs.git"
	}
}
